public class Solution {
    public int Reverse(int x) {
        int result = 0;
        while(x !=0){
            int y = x%10;
            x = x/10;
            //判断是否溢出
            try{
                result = checked (result*10 + y);
            }
            catch(System.OverflowException){
                return 0;
            }
        }
        return result;
    }
}
